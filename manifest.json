{
  "manifest_version": 2,

  "name": "CodeBook",
  "description": "Makes it look like you're coding when you're chatting",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/messages/*", "http://www.facebook.com/messages/*"],
      "js": ["myscript.js"]
    }
  ]
}